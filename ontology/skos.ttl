@prefix : <http://example.org/smartphone/thesaurus#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# =============================================================================
# CONCEPT SCHEMES
# =============================================================================

:ProcessorBrandScheme a skos:ConceptScheme ;
    dct:title "Processor Brand Thesaurus" ;
    skos:hasTopConcept :ProcessorBrand .

:OperatingSystemScheme a skos:ConceptScheme ;
    dct:title "Operating System Thesaurus" ;
    skos:hasTopConcept :MobileOS .

:FeatureScheme a skos:ConceptScheme ;
    dct:title "Smartphone Feature Thesaurus" ;
    skos:hasTopConcept :Feature .

:UseCaseScheme a skos:ConceptScheme ;
    dct:title "Use Case Thesaurus" ;
    skos:hasTopConcept :UseCase .

# =============================================================================
# PROCESSOR BRANDS
# =============================================================================

:ProcessorBrand a skos:Concept ;
    skos:prefLabel "Processor Brand"@en ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:narrower :Snapdragon, :MediaTek, :Exynos, :AppleSilicon, :GoogleTensor .

:Snapdragon a skos:Concept ;
    skos:prefLabel "Snapdragon"@en ;
    skos:altLabel "Qualcomm Snapdragon"@en ;
    skos:broader :ProcessorBrand ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:narrower :Snapdragon8Gen3, :Snapdragon8Gen2, :Snapdragon7Gen1, :Snapdragon6Series .

:Snapdragon8Gen3 a skos:Concept ;
    skos:prefLabel "Snapdragon 8 Gen 3"@en ;
    skos:broader :Snapdragon ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:related :FlagshipProcessor .

:Snapdragon8Gen2 a skos:Concept ;
    skos:prefLabel "Snapdragon 8 Gen 2"@en ;
    skos:broader :Snapdragon ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:related :FlagshipProcessor .

:Snapdragon7Gen1 a skos:Concept ;
    skos:prefLabel "Snapdragon 7 Gen 1"@en ;
    skos:broader :Snapdragon ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:related :MidRangeProcessor .

:Snapdragon6Series a skos:Concept ;
    skos:prefLabel "Snapdragon 6 Series"@en ;
    skos:broader :Snapdragon ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:related :BudgetProcessor .

:MediaTek a skos:Concept ;
    skos:prefLabel "MediaTek"@en ;
    skos:broader :ProcessorBrand ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:narrower :Dimensity9000, :Dimensity8000, :HelioG .

:Dimensity9000 a skos:Concept ;
    skos:prefLabel "Dimensity 9000"@en ;
    skos:broader :MediaTek ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:related :FlagshipProcessor .

:Dimensity8000 a skos:Concept ;
    skos:prefLabel "Dimensity 8000"@en ;
    skos:broader :MediaTek ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:related :MidRangeProcessor .

:HelioG a skos:Concept ;
    skos:prefLabel "Helio G Series"@en ;
    skos:broader :MediaTek ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:related :BudgetProcessor .

:Exynos a skos:Concept ;
    skos:prefLabel "Exynos"@en ;
    skos:altLabel "Samsung Exynos"@en ;
    skos:broader :ProcessorBrand ;
    skos:inScheme :ProcessorBrandScheme .

:AppleSilicon a skos:Concept ;
    skos:prefLabel "Apple Silicon"@en ;
    skos:altLabel "Apple A-Series"@en ;
    skos:broader :ProcessorBrand ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:narrower :A17Pro, :A16Bionic .

:A17Pro a skos:Concept ;
    skos:prefLabel "A17 Pro"@en ;
    skos:broader :AppleSilicon ;
    skos:inScheme :ProcessorBrandScheme ;
    skos:related :FlagshipProcessor .

:A16Bionic a skos:Concept ;
    skos:prefLabel "A16 Bionic"@en ;
    skos:broader :AppleSilicon ;
    skos:inScheme :ProcessorBrandScheme .

:GoogleTensor a skos:Concept ;
    skos:prefLabel "Google Tensor"@en ;
    skos:broader :ProcessorBrand ;
    skos:inScheme :ProcessorBrandScheme .

# Processor tiers (for relating)
:FlagshipProcessor a skos:Concept ;
    skos:prefLabel "Flagship Processor"@en ;
    skos:inScheme :ProcessorBrandScheme .

:MidRangeProcessor a skos:Concept ;
    skos:prefLabel "Mid-Range Processor"@en ;
    skos:inScheme :ProcessorBrandScheme .

:BudgetProcessor a skos:Concept ;
    skos:prefLabel "Budget Processor"@en ;
    skos:inScheme :ProcessorBrandScheme .

# =============================================================================
# OPERATING SYSTEMS
# =============================================================================

:MobileOS a skos:Concept ;
    skos:prefLabel "Mobile Operating System"@en ;
    skos:inScheme :OperatingSystemScheme ;
    skos:narrower :Android, :iOS .

:Android a skos:Concept ;
    skos:prefLabel "Android"@en ;
    skos:broader :MobileOS ;
    skos:inScheme :OperatingSystemScheme ;
    skos:narrower :Android14, :Android13, :Android12 .

:Android14 a skos:Concept ;
    skos:prefLabel "Android 14"@en ;
    skos:broader :Android ;
    skos:inScheme :OperatingSystemScheme .

:Android13 a skos:Concept ;
    skos:prefLabel "Android 13"@en ;
    skos:broader :Android ;
    skos:inScheme :OperatingSystemScheme .

:Android12 a skos:Concept ;
    skos:prefLabel "Android 12"@en ;
    skos:broader :Android ;
    skos:inScheme :OperatingSystemScheme .

:iOS a skos:Concept ;
    skos:prefLabel "iOS"@en ;
    skos:broader :MobileOS ;
    skos:inScheme :OperatingSystemScheme ;
    skos:narrower :iOS17, :iOS16 .

:iOS17 a skos:Concept ;
    skos:prefLabel "iOS 17"@en ;
    skos:broader :iOS ;
    skos:inScheme :OperatingSystemScheme .

:iOS16 a skos:Concept ;
    skos:prefLabel "iOS 16"@en ;
    skos:broader :iOS ;
    skos:inScheme :OperatingSystemScheme .

# =============================================================================
# FEATURES
# =============================================================================

:Feature a skos:Concept ;
    skos:prefLabel "Smartphone Feature"@en ;
    skos:inScheme :FeatureScheme ;
    skos:narrower :Connectivity, :Display, :Camera, :Battery, :Security .

:Connectivity a skos:Concept ;
    skos:prefLabel "Connectivity"@en ;
    skos:broader :Feature ;
    skos:inScheme :FeatureScheme ;
    skos:narrower :5G, :NFC, :Bluetooth, :WiFi, :IR .

:5G a skos:Concept ;
    skos:prefLabel "5G"@en ;
    skos:altLabel "Fifth Generation Network"@en ;
    skos:broader :Connectivity ;
    skos:inScheme :FeatureScheme ;
    skos:definition "Fifth generation mobile network technology"@en .

:NFC a skos:Concept ;
    skos:prefLabel "NFC"@en ;
    skos:altLabel "Near Field Communication"@en ;
    skos:broader :Connectivity ;
    skos:inScheme :FeatureScheme ;
    skos:definition "Short-range wireless technology for contactless payments"@en .

:Bluetooth a skos:Concept ;
    skos:prefLabel "Bluetooth"@en ;
    skos:broader :Connectivity ;
    skos:inScheme :FeatureScheme .

:WiFi a skos:Concept ;
    skos:prefLabel "WiFi"@en ;
    skos:altLabel "Wi-Fi"@en ;
    skos:broader :Connectivity ;
    skos:inScheme :FeatureScheme .

:IR a skos:Concept ;
    skos:prefLabel "IR Blaster"@en ;
    skos:altLabel "Infrared"@en ;
    skos:broader :Connectivity ;
    skos:inScheme :FeatureScheme ;
    skos:definition "Infrared blaster for remote control functionality"@en .

:Display a skos:Concept ;
    skos:prefLabel "Display"@en ;
    skos:broader :Feature ;
    skos:inScheme :FeatureScheme ;
    skos:narrower :AMOLED, :LCD, :HighRefreshRate .

:AMOLED a skos:Concept ;
    skos:prefLabel "AMOLED"@en ;
    skos:altLabel "Active Matrix OLED"@en ;
    skos:broader :Display ;
    skos:inScheme :FeatureScheme .

:LCD a skos:Concept ;
    skos:prefLabel "LCD"@en ;
    skos:altLabel "Liquid Crystal Display"@en ;
    skos:broader :Display ;
    skos:inScheme :FeatureScheme .

:HighRefreshRate a skos:Concept ;
    skos:prefLabel "High Refresh Rate"@en ;
    skos:altLabel "120Hz"@en, "144Hz"@en ;
    skos:broader :Display ;
    skos:inScheme :FeatureScheme ;
    skos:definition "Display refresh rate of 90Hz or higher"@en .

:Camera a skos:Concept ;
    skos:prefLabel "Camera"@en ;
    skos:broader :Feature ;
    skos:inScheme :FeatureScheme ;
    skos:narrower :MultiLens, :HighResolution, :OIS .

:MultiLens a skos:Concept ;
    skos:prefLabel "Multi-Lens Camera"@en ;
    skos:broader :Camera ;
    skos:inScheme :FeatureScheme .

:HighResolution a skos:Concept ;
    skos:prefLabel "High Resolution Camera"@en ;
    skos:altLabel "108MP"@en, "200MP"@en ;
    skos:broader :Camera ;
    skos:inScheme :FeatureScheme .

:OIS a skos:Concept ;
    skos:prefLabel "OIS"@en ;
    skos:altLabel "Optical Image Stabilization"@en ;
    skos:broader :Camera ;
    skos:inScheme :FeatureScheme .

:Battery a skos:Concept ;
    skos:prefLabel "Battery"@en ;
    skos:broader :Feature ;
    skos:inScheme :FeatureScheme ;
    skos:narrower :FastCharging, :WirelessCharging, :LargeBattery .

:FastCharging a skos:Concept ;
    skos:prefLabel "Fast Charging"@en ;
    skos:altLabel "Quick Charge"@en ;
    skos:broader :Battery ;
    skos:inScheme :FeatureScheme .

:WirelessCharging a skos:Concept ;
    skos:prefLabel "Wireless Charging"@en ;
    skos:altLabel "Qi Charging"@en ;
    skos:broader :Battery ;
    skos:inScheme :FeatureScheme .

:LargeBattery a skos:Concept ;
    skos:prefLabel "Large Battery"@en ;
    skos:altLabel "5000mAh+"@en ;
    skos:broader :Battery ;
    skos:inScheme :FeatureScheme .

:Security a skos:Concept ;
    skos:prefLabel "Security"@en ;
    skos:broader :Feature ;
    skos:inScheme :FeatureScheme ;
    skos:narrower :FingerprintSensor, :FaceRecognition .

:FingerprintSensor a skos:Concept ;
    skos:prefLabel "Fingerprint Sensor"@en ;
    skos:broader :Security ;
    skos:inScheme :FeatureScheme .

:FaceRecognition a skos:Concept ;
    skos:prefLabel "Face Recognition"@en ;
    skos:altLabel "Face ID"@en ;
    skos:broader :Security ;
    skos:inScheme :FeatureScheme .

# =============================================================================
# USE CASES - For recommendations
# =============================================================================

:UseCase a skos:Concept ;
    skos:prefLabel "Use Case"@en ;
    skos:inScheme :UseCaseScheme ;
    skos:narrower :Gaming, :Photography, :Business, :BasicUse, :ContentCreation .

:Gaming a skos:Concept ;
    skos:prefLabel "Gaming"@en ;
    skos:broader :UseCase ;
    skos:inScheme :UseCaseScheme ;
    skos:related :HighRefreshRate, :FlagshipProcessor ;
    skos:definition "Mobile gaming requiring high performance"@en .

:Photography a skos:Concept ;
    skos:prefLabel "Photography"@en ;
    skos:broader :UseCase ;
    skos:inScheme :UseCaseScheme ;
    skos:related :HighResolution, :OIS ;
    skos:definition "Mobile photography and videography"@en .

:Business a skos:Concept ;
    skos:prefLabel "Business"@en ;
    skos:broader :UseCase ;
    skos:inScheme :UseCaseScheme ;
    skos:related :NFC, :Security ;
    skos:definition "Professional and business use"@en .

:BasicUse a skos:Concept ;
    skos:prefLabel "Basic Use"@en ;
    skos:broader :UseCase ;
    skos:inScheme :UseCaseScheme ;
    skos:definition "Calls, messaging, browsing"@en .

:ContentCreation a skos:Concept ;
    skos:prefLabel "Content Creation"@en ;
    skos:broader :UseCase ;
    skos:inScheme :UseCaseScheme ;
    skos:related :HighResolution, :AMOLED ;
    skos:definition "Social media and video creation"@en .
